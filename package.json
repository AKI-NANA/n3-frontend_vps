{
  "name": "n3-frontend",
  "version": "0.1.0",
  "private": true,
  "aiRules": "./CLAUDE_RULES.md",
  "codeConventions": "kebab-case-files",
  "scripts": {
    "dev": "next dev -p 3000",
    "dev:turbo": "NODE_OPTIONS='--max-old-space-size=4096' NEXT_TELEMETRY_DISABLED=1 next dev --turbo",
    "dev:light": "NODE_OPTIONS='--max-old-space-size=2048' NEXT_TELEMETRY_DISABLED=1 next dev -p 3001",
    "dev:minimal": "NODE_OPTIONS='--max-old-space-size=1024' NEXT_TELEMETRY_DISABLED=1 next dev -p 3002",
    "vps:local": "cd ~/n3-frontend_vps && npm run dev:3001",
    "vps:local:turbo": "cd ~/n3-frontend_vps && NODE_OPTIONS='--max-old-space-size=4096' NEXT_TELEMETRY_DISABLED=1 next dev --turbo -p 3001",
    "prebuild": "chmod +x scripts/*.sh && scripts/check-build-errors.sh || true",
    "build": "next build",
    "build:clean": "rm -rf .next && next build",
    "build:vps": "rm -rf .next node_modules && npm install && next build",
    "build:safe": "scripts/check-build-errors.sh && next build",
    "start": "next start",
    "lint": "eslint",
    "clean": "rm -rf .next .swc node_modules/.cache",
    "clean:all": "rm -rf .next .swc node_modules node_modules/.cache && npm install",
    "kill": "pkill -9 node",
    "sync:start": "chmod +x scripts/*.sh && scripts/start-auto-sync.sh",
    "sync:stop": "scripts/stop-auto-sync.sh",
    "sync:now": "scripts/auto-sync.sh",
    "vps:setup": "chmod +x scripts/*.sh && scripts/vps-initial-setup.sh",
    "vps:deploy": "chmod +x scripts/*.sh && scripts/deploy-vps.sh",
    "prod:sync": "chmod +x scripts/*.sh && scripts/sync-to-production.sh",
    "prod:deploy": "chmod +x scripts/*.sh && scripts/deploy-all.sh",
    "check:naming": "python3 scripts/check-naming.py",
    "fix:naming": "python3 fix-casing.py",
    "check:imports": "node scripts/check-import-case.js",
    "fix:imports": "node scripts/fix-import-case.js",
    "fix:imports-vps": "node scripts/fix-imports-vps.js",
    "check:vps": "bash scripts/check-vps-ready.sh",
    "lint:naming": "find app lib components -name '*[A-Z]*' -type f 2>/dev/null | head -20 || echo 'No violations found'",
    "precommit": "npm run lint:naming && npm run build",
    "test:vps-compat": "npm run lint:naming && npm run build",
    "unlock-force": "chmod +x scripts/unlock-force.sh && bash scripts/unlock-force.sh",
    "pull-nightly": "chmod +x scripts/pull-nightly.sh && bash scripts/pull-nightly.sh",
    "sync-guard": "chmod +x scripts/sync-guard.sh && bash scripts/sync-guard.sh",
    "empire:setup": "chmod +x scripts/setup-empire-os.sh && bash scripts/setup-empire-os.sh",
    "empire:health": "node governance/ollama-inspector.js --health",
    "empire:preview-start": "pm2 start imperial-preview",
    "empire:preview-stop": "pm2 stop imperial-preview",
    "nightly:run": "node governance/imperial-nightly-engine.js",
    "nightly:dry": "node governance/imperial-nightly-engine.js --dry-run",
    "nightly:watch": "node governance/imperial-nightly-engine.js --watch",
    "audit:full": "node governance/run-full-audit.js --guard --ai",
    "audit:scan": "node governance/total-empire-audit.js"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
    "@google-analytics/data": "^5.2.1",
    "@google/genai": "^1.31.0",
    "@google/generative-ai": "^0.24.1",
    "@hello-pangea/dnd": "^18.0.1",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-checkbox": "^1.1.7",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.58.0",
    "@tailwindcss/postcss": "^4.1.14",
    "@tanstack/react-query": "^5.90.7",
    "@types/ssh2": "^1.15.5",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.21",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "ebay-api": "^9.2.2",
    "firebase": "^12.5.0",
    "glob": "^13.0.0",
    "googleapis": "^167.0.0",
    "gray-matter": "^4.0.3",
    "iconv-lite": "^0.7.2",
    "immer": "^10.2.0",
    "jsonwebtoken": "^9.0.2",
    "jszip": "^3.10.1",
    "lucide-react": "^0.544.0",
    "lz-string": "^1.5.0",
    "moment": "^2.30.1",
    "next": "^15.1.0",
    "p-limit": "^6.2.0",
    "papaparse": "^5.5.3",
    "pdfkit": "^0.17.2",
    "pg": "^8.16.3",
    "playwright": "^1.57.0",
    "puppeteer": "^24.29.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-markdown": "^10.1.0",
    "react-window": "^1.8.10",
    "recharts": "^2.12.7",
    "remark-gfm": "^4.0.1",
    "resend": "^6.6.0",
    "sharp": "^0.34.5",
    "sonner": "^2.0.7",
    "ssh2": "^1.17.0",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^13.0.0",
    "zod": "^3.23.8",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/papaparse": "^5.3.16",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-window": "^1.8.8",
    "dotenv": "^16.6.1",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.14",
    "tsx": "^4.19.2",
    "typescript": "^5"
  }
}
